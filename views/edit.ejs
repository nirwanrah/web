<%- include('partials/header') %>
<h5><b>Edit Data</b></h5>
<form action="" method="post">
    <fieldset disabled>
        <div class="form-group row">
            <input type="checkbox">
            <label for="id" class="col-sm-2 col-form-label">ID</label>

            <div class="col-sm">
                <input type="number" class="form-control" name="id" value="<%= id+1 %>">
            </div>

        </div>
    </fieldset>

    <div class="form-group row">
        <input type="checkbox">
        <label for="string" class="col-sm-2 col-form-label">String</label>

        <div class="col-sm">
            <input type="text" class="form-control" name="string" value="<%= editNth.string %>">
        </div>

    </div>

    <div class="form-group row">
        <input type="checkbox">
        <label for="integer" class="col-sm-2 col-form-label">Integer</label>
        <div class="col-sm">
            <input type="number" class="form-control" name="integer" value="<%= editNth.integer %>">
        </div>
    </div>

    <div class="form-group row">
        <input type="checkbox">
        <label for="float" class="col-sm-2 col-form-label">Float</label>
        <div class="col-sm">
            <input type="float" class="form-control" name="float" value="<%= editNth.float %>">
        </div>
    </div>

    <div class="form-group row">
        <input type="checkbox">
        <label for="date" class="col-sm-2 col-form-label">Date</label>
        <div class="col-sm">
            <input type="date" class="form-control" name="date" value="<%= editNth.date %>">
        </div>
    </div>

    <div class="form-group row">
        <input type="checkbox">
        <label for="boolean" class="col-sm-2 col-form-label">Boolean</label>
        <div class="col-sm">
            <select class="custom-select" name="boolean" value="<%= editNth.boolean %>">
                <option selected>Choose the boolean</option>
                <option value="True">True</option>
                <option value="False">False</option>
            </select>
        </div>
    </div>

    <button type="Submit" class="btn btn-dark">Update</button>

</form>
<br>
<%- include('partials/footer') %>